html, body {
  height: 100%;
}

body {
	font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
}

body {
  display: grid;
  grid-template-columns: 82px 260px 1fr;
  grid-template-rows: 62px 4px 1fr;
  grid-column-gap: 0px;
  grid-row-gap: 0px;
}

.grid-logo { grid-area: 1 / 1 / 2 / 2; }
.grid-header { grid-area: 1 / 2 / 2 / 4; }
.grid-header-border { grid-area: 2 / 1 / 3 / 4; }
.grid-toc { grid-area: 3 / 1 / 4 / 3; }
.grid-body { grid-area: 3 / 3 / 4 / 4; }
.grid-toc, .grid-body {
  overflow: scroll;
}

@media (max-width: 700px) {
  .grid-toc { display: none; }
  .grid-body { grid-area: 3 / 1 / 4 / 4; }
  .hidden-mobile {display: none; }
  .version-tag {display: none; }
  .header-buttons svg, .header-buttons img {
    padding: 8px 0;
    width: 30px;
  }
  .markdown-body h1 {
    font-size: 3.5em;
  }
}

.grid-logo {
  padding: 10px 1em;
}
.grid-header {
  font-family: 'Montserrat', 'ProximaNova-Bold', 'Helvetica Neue', Arial, sans-serif;
  padding: 12px 0.5em 12px 0;
  line-height: 42px;
  letter-spacing: 1px;
  display: flex;
 }
.grid-header h1 {
  font-size: 32px;
  display: inline-block;
  color: #333;
}
.grid-header h1:hover {
  color: #575DFF;
}
.grid-header .version-tag {
  font-size: 20px;
}
.header-buttons a {
  float: right;
  color: #333;
  text-decoration: none;
}
.grid-header-border {
    background: linear-gradient(to right, #55F 0%, #51BBFF66 100%);
 }

.toc {
  padding: 1em;
}
.toc a {
  text-decoration: none;
  color: #888;
}

.toc a.active {
  color: red;
}

.toc ol {
  list-style: none;
  line-height: 1.8;
}
.toc > ol > li {
  font-weight: bold;
}
.toc > ol > li > a {
  color: #333;
}
.toc > ol > li > ol {
  padding: 0 1em 0em 1em;
  font-weight: normal;
}


.grid-body {
  padding: 1em;
  -webkit-overflow-scrolling: touch;
}
.markdown-body {
  max-width: 720px;
  margin-bottom: 50vh;
}

.company-logos {
  display: flex;
  margin-top: -16px;
  margin-bottom: 16px;
}
.company-logos > a {
  text-decoration: none;
  color: #e74c3c;
  height: 116px;
  margin: 8px 4px;
  padding: 16px;
  transition: transform 100ms ease-in-out;
}
.company-logos > .add-company-button {
  display: flex; 
  align-items: center; 
  height: 44px; 
  margin: 44px 8px; 
  border: 2px solid #e74c3c; 
  border-radius: 10px;
  font-weight: 500;
}

img.company-logo {
  box-shadow: none;
  border: none;
  flex: 100%;
  height: 84px;
}
.company-logos > a:hover {
  text-decoration: none;
  font-weight: 500;
  border-radius: 10px;
  box-shadow: 0px 5px 12px 0 #DDD;
  transform: scale(1.02);
}

